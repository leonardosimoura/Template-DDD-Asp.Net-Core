@model IEnumerable<AppTemplate.Domain.Entities.Teste>

<table class="table table-hover">
    <thead>
        <tr>
            <td>
                Nome
            </td>
            <td>
                Email
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Nome
                </td>
                <td>
                    @item.Email
                </td>
            </tr>
        }
    </tbody>
</table>
